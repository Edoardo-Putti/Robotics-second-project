
<launch>
<param name="/use_sim_time" value="true"/>
<node pkg="tf" type="static_transform_publisher" name="optitrack2" args="0 0 0 0 0 0  Robot_1/base_link os1_lidar  100" />

<node pkg="pointcloud_to_laserscan" type="pointcloud_to_laserscan_node" name="pointcloud_to_laserscan">
<remap from="cloud_in" to="/os1_cloud_node/points"/>
<remap from="scan" to="/scan"/>
<rosparam>
transform_tolerance: 0.01
min_height: 0.0
max_height: 4.0
angle_min: -3.14
angle_max: 3.14
angle_increment: 0.0087
scan_time: 0.3333
range_min: 0.45
range_max: 10.0
use_inf: true
concurrency_level: 1
</rosparam>
</node>

<?ignore rosbag play --clock -u 81.0753617286682 peso.bag
?> 

<node pkg="gmapping" type="slam_gmapping" name="gmapping_thing" output="screen" >
  <remap from="scan" to="/scan" />
  <param name="odom_frame" value="world" />
  <param name="base_frame" value="Robot_1/base_link" />
</node>
</launch>
